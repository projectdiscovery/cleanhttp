{
  "services": {
    "cloudflare": {
      "http_status_code": "500-599",
      "http_header": {
        "Server": "cloudflare"
      },
      "http_body": ["error code:"]
    },
    "cloudflare_redirection": {
      "http_status_code": "300-399",
      "http_header": {
        "Server": "cloudflare"
      },
      "check_redirect": {
        "source_ports": [
          8080, 8880, 2052, 2082, 2086, 2095, 2053, 2083, 2087, 2096, 8443
        ],
        "target_ports": [80, 443]
      }
    },
    "cloudflare_ports_blocked_waf": {
      "http_title_regex": [
        "Attention Required! \\| Cloudflare",
        "Just a moment\\.\\.\\."
      ],
      "http_header": {
        "Server": "cloudflare"
      },
      "source_ports": [8080, 8880, 2052, 2082, 2086, 2095, 2053, 2083, 2087, 2096, 8443]
    },
    "cloudfront": {
      "http_status_code": "400",
      "http_header": {
        "Server": "CloudFront"
      },
      "http_title": "ERROR: The request could not be satisfied",
      "http_body": ["Generated by cloudfront (CloudFront)"]
    },
    "akamai": {
      "http_status_code": "400",
      "http_header": {
        "Server": "AkamaiGHost"
      },
      "http_title": "Invalid URL",
      "http_body_regex": ["The requested URL .* is invalid"]
    }
  }
}
